<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mybatis">
    <insert id="insertMaple">
        INSERT INTO maplestory
        VALUES
            #{id}, #{name}, #{level}
    </insert>

    <select id="selectMaple">
        SELECT id,
               name,
               level
        FROM maplestory
    </select>

    <update id="updateMaple">
        UPDATE
            maplesotry
        SET name = #{name}
        WHERE id = #{id}
    </update>


</mapper>